{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/hackathon-api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/hackathon-api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/hackathon-api-gateway",
  "projects": {
    "bridage": {
      "type": "application",
      "root": "apps/bridage",
      "entryFile": "main",
      "sourceRoot": "apps/bridage/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bridage/tsconfig.app.json"
      }
    },
    "contracts": {
      "type": "library",
      "root": "libs/contracts",
      "entryFile": "index",
      "sourceRoot": "libs/contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contracts/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "guards": {
      "type": "library",
      "root": "libs/guards",
      "entryFile": "index",
      "sourceRoot": "libs/guards/src",
      "compilerOptions": {
        "tsConfigPath": "libs/guards/tsconfig.lib.json"
      }
    },
    "hackathon-api-gateway": {
      "type": "application",
      "root": "apps/hackathon-api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/hackathon-api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hackathon-api-gateway/tsconfig.app.json"
      }
    },
    "strategy": {
      "type": "library",
      "root": "libs/strategy",
      "entryFile": "index",
      "sourceRoot": "libs/strategy/src",
      "compilerOptions": {
        "tsConfigPath": "libs/strategy/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "workouts": {
      "type": "application",
      "root": "apps/workouts",
      "entryFile": "main",
      "sourceRoot": "apps/workouts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/workouts/tsconfig.app.json"
      }
    }
  }
}